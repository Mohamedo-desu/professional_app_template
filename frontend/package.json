{
  "name": "professional_template_app",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "build:android": "clear && eas build --platform android --profile preview",
    "offline": "expo start --offline",
    "publish:expo": "clear && eas update --branch preview",
    "build:web": "clear && npx expo export --platform web && eas deploy --prod",
    "release:web": "clear && eas deploy --prod",
    "upgrade": "clear && bun install expo@latest && bunx expo install --fix",
    "android": "clear && bunx expo run:android --device --variant debug",
    "release:android": "bunx expo run:android --device --variant Release",
    "release:ios": "bunx expo run:ios --configuration Release",
    "test": "jest --watchAll",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.15.0",
    "@dicebear/adventurer": "^9.2.4",
    "@dicebear/core": "^9.2.4",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@faker-js/faker": "^10.0.0",
    "@legendapp/list": "^2.0.6",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@sentry/react-native": "~7.1.0",
    "convex": "1.27.3",
    "convex-helpers": "^0.1.104",
    "date-fns": "^4.1.0",
    "expo": "54.0.12",
    "expo-auth-session": "~7.0.8",
    "expo-build-properties": "^1.0.8",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.10",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "^6.0.7",
    "expo-updates": "~29.0.12",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-animated-numbers": "^0.6.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-heroicons": "^4.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^3.3.3",
    "react-native-nitro-modules": "^0.29.6",
    "react-native-reanimated": "3.19.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-unistyles": "^3.0.13",
    "svix": "^1.76.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
